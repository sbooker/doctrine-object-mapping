{
  "name": "sbooker/doctrine-object-mapping",
  "description": "Doctrine object types mapping support",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Knigin",
      "email": "sergey.knigin.dev@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "doctrine/dbal": "^3.0",
    "symfony/serializer": "^6.4 | ^7.0",
    "symfony/http-kernel": "^6.4 | ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "Sbooker\\DoctrineObjectMapping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Sbooker\\DoctrineObjectMapping\\": "tests/"
    }
  }
}
